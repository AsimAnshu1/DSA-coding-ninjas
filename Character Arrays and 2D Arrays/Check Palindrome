int length(char str[]){
    int count =0;
    for(int i=0;str[i]!='\0';i++){
        count++;
    }
    return count;
}
bool checkPalindrome(char str[]) {
    int s=0;
    int e = length(str)-1;
    while(s<=e){
        if(str[s]!=str[e]){
            return false;
        }
        s++;
        e--;
        }   
}

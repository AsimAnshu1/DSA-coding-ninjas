#include<iostream>
using namespace std;
int main() {
    
    /*  Read input as specified in the question.
     * Print output as specified in the question.
     */
    int n;
    cin>>n;
    char startchar='A'+n;
    for(int i=1;i<=n;i++){
        for(int j=i;j>=1;j--){
            char ch=startchar-j;
            cout<<ch;
           // ch = ch+1;
		}
        cout<<endl;
     }
}

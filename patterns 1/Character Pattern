#include<iostream>
using namespace std;
int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
  int n;
   // char ch ='A';
    cin>>n;
    for(int i=1;i<=n;i++){
        char startchar='A'+i-1;
        for(int j=1;j<=i;j++){
            char ch=startchar+j-1;
            cout<<ch;
           // ch = ch+1;
		}
        cout<<endl;
    }
}

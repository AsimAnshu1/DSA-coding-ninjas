#include<iostream>
#include<cmath>
using namespace std;

int main(){
    int num ;
    int n;
    cin>>num>>n;
    int i = 1;
    int y = num;
    
     if(n==0){
            cout<<"1";
        }
    else{
    while(i<=n-1){
       
       
        y = y*num;
        i = i+ 1;
    }
        cout<<y;
    }
  
}

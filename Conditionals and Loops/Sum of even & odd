#include<iostream>
using namespace std;

int main() {
	// Write your code here
	int n;
    cin>>n;
    int even =0;
    int odd=0;
    int digit =0;
    while(n!=0){
        digit= n%10;
        if(digit%2==0){
            even = even+digit;
        }
        else{
            odd = odd+digit;
        }
        n = n/10;
    }
    cout<<even<<" "<<odd;
}
